{
	"scripts": {
		"start": "cross-env NODE_ENV_PRODUCTION=NO NODE_ENV_SOURCE_MAP=YES webpack serve --mode development --hot --open",
		"build-sourcemap": "cross-env NODE_ENV_PRODUCTION=YES NODE_ENV_SOURCE_MAP=YES webpack --config webpack.config.js",
		"build": "cross-env NODE_ENV_PRODUCTION=YES NODE_ENV_SOURCE_MAP=NO webpack --config webpack.config.js",
		"test": "jest"
	},
	"babel": {
		"env": {
			"test": {
				"plugins": [
					"transform-es2015-modules-commonjs"
				]
			}
		}
	},
	"jest": {
		"verbose": true,
		"transform": {
			"^.+\\.js": "<rootDir>/node_modules/babel-jest"
		},
		"moduleFileExtensions": [
			"js"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.16.0",
		"babel-loader": "^8.2.3",
		"cross-env": "^7.0.3",
		"css-loader": "^6.5.1",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^19.0.0",
		"jest": "^27.4.5",
		"sass": "^1.75.0",
		"sass-loader": "^12.3.0",
		"style-loader": "^3.3.1",
		"webpack": "^5.64.4",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.6.0",
		"worker-loader": "^3.0.8"
	},
	"dependencies": {
		"ccapture.js": "1.1.0",
		"cm-show-invisibles": "2.0.2",
		"codemirror": "5.59.4",
		"codemirror-formatting": "1.0.0",
		"collect-fps": "2.0.0",
		"hermite-resize": "git+https://github.com/viliusle/Hermite-resize.git",
		"jquery": "3.6.0",
		"keymaster": "1.6.2",
		"localforage": "1.9.0",
		"normalize.css": "8.0.1",
		"vue": "2.6.12"
	}
}
